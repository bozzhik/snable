{
  "name": "snable",
  "displayName": "Snable: UI Inspector & CSS Scanner",
  "version": "0.2.3",
  "author": "bozzhik",
  "description": "Analyze any web page that catches your eye! Get a detailed overview of colors, fonts, and visual elements for design inspiration.",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "chrome-extension",
    "react",
    "vite",
    "create-chrome-ext"
  ],
  "engines": {
    "node": ">=14.18.0"
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "tsc && vite build",
    "lint": "tsc",
    "fmt": "prettier --write '**/*.{tsx,ts,json,css,scss,md}'",
    "zip": "pnpm run build && node src/zip.js"
  },
  "dependencies": {
