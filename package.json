    "lucide-react": "^0.475.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sonner": "^2.0.1",
    "wouter": "^3.6.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.12",
    "@crxjs/vite-plugin": "^2.0.0-beta.19",
    "@tailwindcss/postcss": "^4.0.1",
    "@types/chrome": "^0.0.246",
    "@types/node": "^22.13.9",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@vitejs/plugin-react": "^4.1.0",
    "clsx": "^2.1.1",
    "gulp": "^4.0.2",
    "gulp-zip": "^6.0.0",
    "postcss": "^8.5.1",
    "prettier": "^3.0.3",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^4.0.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.11"
  }
